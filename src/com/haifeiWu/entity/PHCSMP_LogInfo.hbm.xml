<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.manageplantfrom.entity">

<!-- 日志信息表 -->
    <class name="PHCSMP_LogInfo" table="PHCSMP_LogInfo" lazy="false">
    
        <id name="Log_ID">
            <generator class="native"/>
        </id>
        <property name="Operation_Time"></property>
        <property name="Staff_Name"/>
        <property name="Operation_Info"/>
        <property name="Operation_Model"/>
    </class>
        

</hibernate-mapping>
