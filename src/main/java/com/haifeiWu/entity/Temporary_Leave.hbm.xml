<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.haifeiWu.entity">

    <class name="Temporary_Leave" table="Temporary_Leave" lazy="false">
    
        <id name="temporary_Leave_Id">
            <generator class="native"/>
        </id>
        
        <property name="suspect_ID"></property>
        <property name="tempLeave_Time"/>
        <property name="tempLeave_Reason"/>
        <property name="return_Time"/>
        
        <property name="return_staff_ID"/>
         <property name="tempLeave_staff_ID"/>
        <property name="room_ID"/>
        <property name="tempLeave_manager"></property>
        <property name="return_manager"></property>
    </class>

</hibernate-mapping>
